{
  "version": 2,
  "builds": [
    {
      "src": "index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    { "src": "/api/(.*)", "dest": "/api/$1", "methods": ["GET", "POST", "PUT", "DELETE"] },
    { "src": "/api/(.*)", "status": 404, "dest": "/api/404" },
    { "src": "/(.*)", "dest": "/index.js" }
  ]
}
